<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="CCVProyecto2P2.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:CCVProyecto2P2.Views"
  
    Title="CCVProyecto2P2">

    <FlyoutItem Title="Inicio">
        <ShellContent
        ContentTemplate="{DataTemplate local:InicioView}"
        Route="InicioView" />
    </FlyoutItem>
    <FlyoutItem Title="Soporte">
        <ShellContent
        
        ContentTemplate="{DataTemplate local:SoporteView}"
        Route="SoporteView" />
    </FlyoutItem>

</Shell>
